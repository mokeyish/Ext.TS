{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "unified-signatures": false,
        "no-console": false,
        "no-conditional-assignment": false,
        "one-variable-per-declaration": false,
        "only-arrow-functions": false,
        "no-empty-interface": false,
        "max-classes-per-file": false,
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-separator",
            "check-rest-spread",
            "check-type",
            "check-typecast",
            "check-type-operator",
            "check-preblock",
            "check-postbrace"
        ],
        "quotemark": [
            true,
            "single"
        ]
    },
    "rulesDirectory": []
}
